{
    "name": "northwoods/broker",
    "description": "A conditional dispatcher for PSR-15 middleware",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Woody Gilk",
            "email": "woody.gilk@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "eloquent/phony-phpunit": "^3.0",
        "phpunit/phpunit": "^6.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "Northwoods\\Broker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Northwoods\\Broker\\": "tests/"
        }
    }
}
